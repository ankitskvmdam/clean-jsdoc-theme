{
    "source": {
        "include": [""],
        "includePattern": ".(jsx|js|ts|tsx)$",
        "excludePattern": "(node_modules/)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    },
    "tags": {
        "allowUnknownTags": true
    },
    "opts": {
        "recurse": true,
        "verbose": true,
        "destination": "./docs",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "sections": ["Classes", "Modules", "Global", "Menu", "Tutorials"],
            "theme": "dark",
            "favicon": "https://dl.dropbox.com/s/unb7kozlrbox72t/favicon.ico",
            "tutorials": "./src/tutorials",
            "add_style_path": [{
                "href": "https://dl.dropbox.com/s/tljrmo925z4jgdi/professional.css"
            }],
            "add_scripts": "function foo(){console.log('foo')}",
            "add_script_path": [{
                "src": "https://code.jquery.com/jquery-3.5.1.js",
                "integrity": "sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=",
                "crossorigin": "anonymous"
            }],
            "codepen": {
                "enable_for": ["examples"],
                "options": {
                    "js_external": "https://code.jquery.com/jquery-3.6.0.min.js",
                    "js_pre_processor": "babel"
                }
            }
        }
    }
}